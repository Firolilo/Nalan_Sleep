text_list[0] = "Siempre has sido bueno escondiendote, Nalan. Creaste este lugar para huir de la verdad."
text_list[1] = "Delante de ti hay dos puertas."
text_list[2] = "La izquierda te acoge en la mentira, la derecha es la cruda verdad."
text_list[3] = "Que eliges, Nalan? Seguir escondido, o enfrentar lo que hiciste?"
